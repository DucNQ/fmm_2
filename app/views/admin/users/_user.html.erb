<tr>
  <td><%= link_to user.name, edit_admin_user_path(user) %></td>
  <td><%= user.email %></td>
  <td><%= user.birthday.to_date %></td>
  <td><%= user.position.abbr unless user.position.nil? %></td>
  <td><%= user.team.name unless user.team.nil? %></td>
  <td><%= link_to "x", admin_user_path(user), method: :delete, data: {confirm: "You sure?"} %></td>
</tr>